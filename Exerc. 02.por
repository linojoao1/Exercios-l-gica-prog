programa {
  funcao inicio() {
    inteiro num
    inteiro num2


    escreva("digite o primeiro número inteiro ")
    leia(num)
    escreva("digite o segundo número inteiro ")
    leia(num2)
     
     se(num==num2){
      escreva(" numeros iguais")
